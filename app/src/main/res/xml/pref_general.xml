<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"

    >
    <EditTextPreference

        android:id="@+id/edit_Text_Preference"
        android:title="@string/pref_general_title"

        android:key="@string/pref_general_key"
        android:defaultValue="@string/pref_general_value"
        android:inputType="text"
        android:singleLine="true"
        />

    <ListPreference
        android:title="@string/Pref_temp_title"
        android:key="@string/Pref_temp_unit_key"
        android:defaultValue="@string/Pref_temp_unit_defult_metric"
        android:entryValues="@array/entryvalue"
        android:entries="@array/entries"

        />






</PreferenceScreen>